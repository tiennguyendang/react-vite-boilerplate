{
  "name": "react-vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,scss,css}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --quiet"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "vite": "^2.9.9",
    "vite-plugin-environment": "^1.1.1"
  },
  "engines": {
    "npm": "please use yarn",
    "yarn": ">= 1.19.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{js,jsx,ts,tsx,scss,css}": [
      "yarn format"
    ]
  }
}
